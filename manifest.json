{
        "name": "Redable Trello",
        "version": "0.1",
        "manifest_version": 2,
        "description": "A Clean Readable Trello User Interface",
        "homepage_url": "http://belkinserge.com",
        "icons": { "16": "icon16.png", "48": "icon48.png", "128": "icon128.png" },
        "content_scripts": [
        {
            "matches": ["http://trello.com/*","https://trello.com/*"],
            "css": ["readabletrello.css"]
        }
    ] 
}